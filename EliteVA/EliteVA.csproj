<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="EliteAPI" Version="3.1.1" />
      <PackageReference Include="EliteAPI.Web" Version="3.1.1" />
      <PackageReference Include="EliteAPI.Web.Spansh" Version="3.1.1" />
      <PackageReference Include="Microsoft.Extensions.Configuration.Ini" Version="7.0.0" />
      <PackageReference Include="WatsonWebsocket" Version="4.0.11" />
    </ItemGroup>

    <ItemGroup>
      <None Update="layout.yml">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="EliteVA.ini">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>
</Project>
